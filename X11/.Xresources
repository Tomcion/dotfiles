Xft.dpi: 164
Xft.autohint: 0
Xft.hintstyle:  hintfull
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb

URxvt.scrollBar: false

#define FONTSIZE 12
#define FONTNAME JetBrainsMono Nerd Font

URxvt*font:     xft:FONTNAME:size=FONTSIZE:antialias=true:hinting=true:autohint=false:hintstyle=hintnone:dpi=164
URxvt*boldFont: xft:FONTNAME:size=FONTSIZE:antialias=true:hinting=true:autohint=false:hintstyle=hintnone:dpi=164

! Grey
*color0: #666666
*color8: #666666

! Red
*color1:    #cc6666
*color9:    #d54e53

! Green
*color2:    #b5bd68
*color10:    #b9ca4a

! Yellow
*color3:    #f0c674
*color11:    #e7c547

! Blue
*color4:    #81a2be
*color12:    #7aa6da

! Magenta
*color5:    #b294bb
*color13:    #c397d8

! Cyan
*color6:    #8abeb7
*color14:    #70c0b1

! White
*color7:    #c5c8c6
*color15:    #eaeaea

! Some colors
*background:    #1d1f21
*foreground:    #c5c8c6
*cursorColor:   #c5c8c6

#define BARFONT Hack Nerd Font:size=10 

#define BG #222222
#define SELBG #bd7777
/* #define SELBG #81a2be */

#define FG #bbbbbb
#define SELFG #222222

dwm.font: BARFONT

dwm.normbgcolor:       BG
dwm.selbgcolor:        SELBG

dwm.normfgcolor:       FG
dwm.selfgcolor:        SELFG

dwm.normbordercolor:   #444444
dwm.selbordercolor:    #005577

dmenu.font: BARFONT

dmenu.background: BG
dmenu.foreground: FG

dmenu.selbackground: SELBG
dmenu.selforeground: SELFG
